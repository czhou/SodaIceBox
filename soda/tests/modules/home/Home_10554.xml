
<soda>
	<puts text="Test:home_10554 Add my quotes dashlet" />
		
	<!--create quote-->
	<script file="{@global.scriptsdir}/modules/lib/Quote_Create_Lib.xml" />
	<csv file="{@global.scriptsdir}/modules/lib/Quote_Create_Lib.csv" var="home10554">
		<!--check the my quotes dashlet-->
		<link id="moduleTab_Home" />
		<browser assert="Add Sugar Dashlets" />
		<!--Add dashlet-->
		<link id="add_dashlets" />
		<wait timeout="5" />
		<div id="dashletsDialog_c">
			<browser assert="Find Sugar Dashlet" />
			<link text="My Quotes" />
			<wait timeout="5" />	
		</div>
		
		<ul id="col_0_0">			
			<li index="1">
				<browser assert="My Quotes" />
				<table class="list view">
						<link text="{@pre_Quote_name}" click="false" />
						<link text="{@pre_account_name}" click="false" />
						<browser assert="{@home10554.quote_stage}" />			
				</table>
			</li>
		</ul>
		
		<!-- delete dashlet -->
		<div class="dashletToolSet" index="1">
			<link index="3" alert="true" />
		</div>
	</csv>
	
		
	<!-- delete records-->	
	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Quote_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/TaxRate_Delete_Lib.xml" />
    <script file="{@global.scriptsdir}/modules/lib/ProductCatalog_Delete_Lib.xml" />
      
	
	<puts text="Test: home_10554.xml finished." />
</soda>	
