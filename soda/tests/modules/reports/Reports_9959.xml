<soda>
	<puts text="Test Case 9959: Check new options when define filters for TextArea/TextField/URL/Phone fields in report" />

	<!--goto Reports module-->
	<link id="moduleTab_Reports" />
	<link text="Create Report" />
	<browser assert="Report Wizard" />

	<image name="rowsColsImg" click="true"/>
	<wait timeout="3"/>
	<table id="Accounts" click="true"/>
	<wait timeout="2"/>	
	<row id="yui-rec9" click="true" />		<!-- Description -->

	<wait timeout="1"/>	
	<row id="Filter.1_table_filter_row_1">
		<cell index="3">
			<select index="1" assert="Equals" />
			<select index="1" assert="Does Not Equal" />
			<select index="1" assert="Contains" />
			<select index="1" assert="Does Not Contain" />
			<select index="1" assert="Starts With" />
			<select index="1" assert="Ends With" />
			<select index="1" assert="Is Empty" />
			<select index="1" assert="Is Not Empty" />
		</cell>
	</row>
	
	<!--New Report for name...-->
	<link id="moduleTab_Reports" />
	<wait timeout="5"/>
	<link text="Create Report" />
	<browser assert="Report Wizard" />

	<image name="rowsColsImg" click="true"/>
	<wait timeout="2"/>
	<table id="Accounts" click="true"/>
	<wait timeout="2"/>
	<row id="Accounts_name" click="true" />		<!-- Name -->
	<wait timeout="1"/>	
	<row id="Filter.1_table_filter_row_1">
		<cell index="3">
			<select index="1" assert="Is" />
			<select index="1" assert="Is Not" />
			<select index="1" assert="Equals" />
			<select index="1" assert="Does Not Equal" />
			<select index="1" assert="Contains" />
			<select index="1" assert="Does Not Contain" />
			<select index="1" assert="Starts With" />
			<select index="1" assert="Ends With" />
			<select index="1" assert="Is Empty" />
			<select index="1" assert="Is Not Empty" />
		</cell>
	</row>
	
	<!--New Report for Office Phone...-->
	<link id="moduleTab_Reports" />
	<wait timeout="5"/>
	<link text="Create Report" />
	<browser assert="Report Wizard" />

	<div id="report_type_div">
		<table index="1">
			<row index="2">
				<cell index="1">
					<table index="1">
						<row index="1">
							<cell index="1">
								<image index="1" click="true" />
							</cell>
						</row>
					</table>
				</cell>
			</row>
		</table>
	</div>
	
	<table id="Accounts" jscriptevent="onclick" />
	<wait timeout="1"/>	
	<row id="yui-rec17" click="true" />		<!-- Office Phone -->
	
	<row id="Filter.1_table_filter_row_1">
		<cell index="3">
			<select index="1" assert="Equals" />
			<select index="1" assert="Does Not Equal" />
			<select index="1" assert="Contains" />
			<select index="1" assert="Does Not Contain" />
			<select index="1" assert="Starts With" />
			<select index="1" assert="Ends With" />
			<select index="1" assert="Is Empty" />
			<select index="1" assert="Is Not Empty" />
		</cell>
	</row>
	
	<!--New Report for Web Site...-->
	<link id="moduleTab_Reports" />
	<wait timeout="5"/>
	<link text="Create Report" />
	<browser assert="Report Wizard" />

	<div id="report_type_div">
		<table index="1">
			<row index="2">
				<cell index="1">
					<table index="1">
						<row index="1">
							<cell index="1">
								<image index="1" click="true" />
							</cell>
						</row>
					</table>
				</cell>
			</row>
		</table>
	</div>
	
	<table id="Accounts" jscriptevent="onclick" />
	
	<row id="yui-rec26" click="true" />		<!-- Web Site -->
	<wait timeout="1"/>	
	<row id="Filter.1_table_filter_row_1">
		<cell index="3">
			<select index="1" assert="Equals" />
			<select index="1" assert="Does Not Equal" />
			<select index="1" assert="Contains" />
			<select index="1" assert="Does Not Contain" />
			<select index="1" assert="Starts With" />
			<select index="1" assert="Ends With" />
			<select index="1" assert="Is Empty" />
			<select index="1" assert="Is Not Empty" />
		</cell>
	</row>

	<puts text="Test: Reports / 9959 finished." />
</soda>
