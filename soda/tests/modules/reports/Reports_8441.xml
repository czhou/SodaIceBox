
<soda>
	<!-- filter data field without data seletor -->
	<puts text="Test: Reports_8441.xml starting" />
	<timestamp />

	<!--goto Report module -->
	<link id="moduleTab_Reports" />
	<link text="Create Report" />

	<!--create row and column report -->
	<image name="rowsColsImg" click="true" />
	<table id="Contacts" >
		<link text="Contacts" alert="true"/>
	</table>

	<wait timeout="3" />
	<div text="Birthdate" click="true" />
	<wait timeout="2" />
	<div text="Date Created" click="true" />
	<wait timeout="2" />
	<div text="Date Modified" click="true" />
	<wait timeout="2" />

	<div id="filter_designer_div">
		<select name="qualify" index="1" set="On" />
		<textfield id="jscal_field" index="1" set="12/31/2009" />

		<select name="qualify" index="2" set="Not On" />
		<textfield id="jscal_field" index="2" set="12/31/2009" />

		<select name="qualify" index="3" set="After" />
		<textfield id="jscal_field" index="3" set="12/31/2009" />
	</div>
	<button name="Next &gt;" />

	<div text="Birthdate" click="true" />
	<div text="Date Created" click="true" />
	<div text="Date Modified" click="true" />

	<button name="Next &gt;" />

	<csv file="{@global.scriptsdir}/csvs/reports/Reports_7734.csv" var="report_record_8441">
		<textfield name="save_report_as" set="{@report_record_8441.save_report_as} {@stamp}" />
	</csv>

	<button name="Save and Run" />
	<wait />
	<browser assert="Title: {@report_record_8441.save_report_as} {@stamp}" />
	
	<!-- Cleanup: Delete created Report. -->
	<script file="{@global.scriptsdir}/modules/lib/Report_Delete_Lib.xml" />

	<puts text="Test: Reports_8441.xml finished" />
</soda>
