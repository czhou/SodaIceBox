
<soda>
	<puts text="Test Case 11523: Verify import settings are kept after back to the first step of wizard."/>

	<link id="moduleTab_Contacts" />
	<csv file="{@global.scriptsdir}/csvs/imports/Imports_11523.csv" var="import_11523">

		<link text="Import Contacts" />

		<!--Step1-->
		<div id="importWizardDialogTitle">
			<browser assert="Step 1: Select Data Source" />
		</div>
		<button id="gonext"/>

		<!--Step2-->
		<div id="importWizardDialogTitle">
			<browser assert="Step 2: Upload Import File" />
		</div>

		<filefield name="userfile" set="{@global.scriptsdir}/csvs/imports/Imports_11523.csv" />
		<button id="gonext" />
		<wait timeout="5" />

		<!--Step3-->
		<div id="importWizardDialogTitle">
			<browser assert="Step 3: Confirm Import File Properties" />
		</div>

		<button id="toggleImportOptions" />
		<textfield id="custom_delimiter" assert=","/>
		<select id="custom_enclosure" assert="Single Quote (')"/>
		<select id="custom_enclosure" set="None"/>
		<button id="gonext" />
	
		<!--Step4-->
		<div id="importWizardDialogTitle">
			<browser assert="Step 4: Confirm Field Mappings" />
		</div>
		<select name="colnum_0" set="last_name"/>
		<button id="goback" />
		
		<!--Step3-->
		<div id="importWizardDialogTitle">
                        <browser assert="Step 3: Confirm Import File Properties" />
                </div>

                <button id="toggleImportOptions" />
                <textfield id="custom_delimiter" assert=","/>
                <select id="custom_enclosure" assert="None"/>
                <button id="gonext" />	
		
		<div class="moduleTitle">
                        <browser assert="Step 4: Confirm Field Mappings" />
                </div>
                <select name="colnum_0" set="last_name"/>
                <button id="gonext" />
	
		<!--Step5-->
		<button id="importnow" />
		<wait timeout="10" />

		<!--Step6-->
		<div id="importWizardDialogTitle">
			<browser assert="Step 6: View Import Results" />
		</div>
		<div id="pageNumIW_0_div">
                        <span class="pageNumbers" assert="(1 - 1 of 1)"/>
                </div>

	</csv>

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Contact_Delete_Lib.xml" />
	<puts text="Test: Imports_11523.xml finished." />
</soda>
