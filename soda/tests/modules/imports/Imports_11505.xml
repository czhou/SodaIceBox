
<soda>
	<puts text="Test Case 11505: Verify qualifier can be changed in confirmation page."/>

	<link id="moduleTab_Contacts" />
	<csv file="{@global.scriptsdir}/csvs/imports/Imports_11505.csv" var="import_11505">

		<link text="Import Contacts" />

		<!--Step1-->
		<div id="importWizardDialogTitle">
			<browser assert="Step 1: Select Data Source" />
		</div>
		<button id="gonext"/>

		<!--Step2-->
		<div id="importWizardDialogTitle">
			<browser assert="Step 2: Upload Import File" />
		</div>

		<filefield name="userfile" set="{@global.scriptsdir}/csvs/imports/Imports_11505.csv" />
		<button id="gonext" />
		<wait timeout="5" />

		<!--Step3-->
		<div id="importWizardDialogTitle">
			<browser assert="Step 3: Confirm Import File Properties" />
		</div>

		<button id="toggleImportOptions" />
		<textfield id="custom_delimiter" assert=","/>
		<select id="custom_enclosure" assert="Single Quote (')"/>
		<select id="custom_enclosure" set="None"/>
		<button id="gonext" />
	
		<!--Step4-->
		<div id="importWizardDialogTitle">
			<browser assert="Step 4: Confirm Field Mappings" />
		</div>
		<select name="colnum_0" set="last_name"/>
		<button id="gonext" />
		
		<!--Step5-->
		<button id="importnow" />
		<wait timeout="10" />

		<!--Step6-->
		<div id="importWizardDialogTitle">
			<browser assert="Step 6: View Import Results" />
		</div>

		<!--Search the record to assert import successfully -->
		<link id="moduleTab_Contacts" />
		<div class="moduleTitle">
			<link href="index.php?module=Contacts&amp;action=index" click="false" />
		</div>

		<textfield id="search_name_basic" set="{@import_11505.last_name}" />
		<button id="search_form_submit" />
		<table class="list view">
			<link text="{@import_11505.first_name} {@import_11505.last_name}" />
		</table>
	</csv>

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Contact_Delete_Lib.xml" />
	<puts text="Test: Imports_11505.xml finished." />
</soda>
