
<soda>
	<!-- Delete Lead_Verify that lead can be deleted in 'Lead' detail view. -->
	<puts text="Test: Leads_1901.xml starting." />

	<script file="{@global.scriptsdir}/modules/lib/Lead_Create_Lib.xml"/>

	<!-- Go to Lead module -->
	<link id="moduleTab_Leads" />
	<div class="moduleTitle">
		<link href="index.php?module=Leads&amp;action=index" click="false" />
	</div>

	<!--Go to detail view and delete the record-->
	<link
		text="{@Lead_New_Record.salutation} {@pre_lead_first_name} {@pre_lead_last_name}" />
	<button value="Delete" alert="true" />
	<div class="moduleTitle">
		<link href="index.php?module=Leads&amp;action=index" click="false" />
	</div>

	<link
		text="{@Lead_New_Record.salutation} {@pre_lead_first_name} {@pre_lead_last_name}"
		click="false" exist="false" />

	<puts text="Test: Leads_1901.xml finished" />
</soda>
