<soda>
	<puts text="Test: Reports_8598.xml starting." />
	<!--

	Action:
		1. Login with valid user.
		2. Create a custom Summation Report with Details.
		3. Run the report.
		4. Click the collapse icon in the report result.
		   /* mfleeger - no longer exists... */

	Expected Results:
		The Summation details are collapsed.
		/* mfleeger - appropriate results are displayed instead */

	-->

	<script file="{@global.scriptsdir}/modules/lib/Account_Create_Lib.xml" />

	<link id="moduleTab_Reports" />
	
	<link text="Create Report" />

	<wait timeout="4" />
		

    <div id="report_type_div">
        <image name="summationImg" jscriptevent="onclick" click="true" />
    </div>

	<table id="Accounts">
		<link text="Accounts" />
	</table>
	

	<wait timeout="4" />
	
	<button value="Next &gt;" />
	<button value="Next &gt;" />

	
	<div text="Count" click="true" />
	
	<button value="Next &gt;" />
	<button value="Next &gt;" />

	
	<timestamp />
	<var var="report_title" set="QA_Created_{@stamp}" />
	<textfield name="save_report_as" set="{@report_title}" />
	
	<button value="Save" />
	
	<wait timeout="4" />

	<link id="moduleTab_Reports" />
	
	<textfield name="name_basic" set="{@report_title}" />
	<button id="search_form_submit" />
	
	<javascript>
		stars = document.getElementsByClassName('off');
		var star = stars[0];
		star.onclick();
	</javascript>
	
	<link text="My Favorite Reports" />
	
	<browser assert="{@report_title}" />
	
	<script file="{@global.scriptsdir}/modules/lib/Report_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />
		
	<puts text="Test: Reports_8598.xml finished." />
</soda>	
