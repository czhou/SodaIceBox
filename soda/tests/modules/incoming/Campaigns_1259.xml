<soda>
	<puts text="Test: Campaigns_1259.xml starting." />
	<!--
	Action:
		1. Enter the first letter of a target's "LastName" (the target exists).

	Expected:
		1. The target records with first letter(LastName) entered is displayed
			in the targets view.
	-->
	
	<script file="{@global.scriptsdir}/modules/lib/Target_Create_Lib.xml" />
	<var var="target1_name" set="{@Target_First_Name} {@Target_Last_Name}" />

	<link id="moduleTab_Prospects" />
	
	<textfield id="search_name_basic" set="C" />
	<button id="search_form_submit" />
	
	<table class="list view">
		<browser assert="{@target1_name}" />
	</table>
	
	<button id="search_form_clear" />
	<button id="search_form_submit" />
	
	<script file="{@global.scriptsdir}/modules/lib/Target_Delete_Lib.xml" />
	
	<puts text="Test: Campaigns_1259.xml finished." />
</soda>	