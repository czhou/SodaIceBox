<soda>
	<!--
		Action:
			1. Login to system as a valid user.
			2. Click "Calendar" navigation tab.
			3. Click "Schedule Call" link on Navigation shortcuts.
			4. Enter all the Mandatory fields, like "Subject",
				"Start date & time", "Team", "Status" and "Duration".
			5. Click on "Cancel" button.
		Expectation:
			5. The canceled call is not displayed in "Call List" sub-panel.
	-->
	<puts text="Test: Calendar_3612.xml starting." />
	
	<link id="moduleTab_Calendar" />

	<link text="Log Call" />
	
	<timestamp />
	<var var="call_name" set="Call_Temp {@stamp}" />
	
	<textfield id="name" set="{@call_name}" />
	
	<button value="Cancel" alert="true" />
	
	<link id="moduleTab_Calls" />
	<table class="list view">
		<browser assertnot="{@call_name}" />
	</table>
	
	<puts text="Test: Calendar_3612.xml finished." />
</soda>