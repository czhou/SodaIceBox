<soda>
	<!--
		Action:
			1. Login to system as a valid user.
			2. Click "Calendar" navigation tab.
			3. Click "Meetings" link on Navigation shortcuts.
			2,3. Meetings is its own tab now - mfleeger
			4. Click any "Close" icon for a meeting in "Meeting List" view.
		Expectation:
			4. Meeting edit page is displayed and the meeting status
				change to "Held" automatically.
			4. mfleeger - this is now handled via AJAX.  Will check status
				via DetailView...
	-->
	<puts text="Test: Calendar_0702.xml starting." />
	
	<script file="{@global.scriptsdir}/modules/lib/Meeting_Create_Lib.xml" />
	
	<link id="moduleTab_Meetings" />
	
	<textfield id="name_basic" set="{@pre_meeting_name}" />
	<button id="search_form_submit" />
	
	<browser assert="{@pre_meeting_name}" />
	
	<javascript loadUtils="true">
		rows = document.getElementsByClassName("oddListRowS1");
		nodes = rows[0].children;
		the_td = nodes[3];
		links = the_td.children;
		links[1].onclick();
	</javascript>
	
	
	<!-- check for dialog box -->
	<div id="closeActivityDialog">
		<browser assert="Confirm" />
	</div>
	
	<button text="OK" />
	
	<link text="{@pre_meeting_name}" />
	
	<div id="Meetings_detailview_tabs">
		<browser assert="Held" />
	</div>
	
	<script file="{@global.scriptsdir}/modules/lib/Meeting_Delete_Lib.xml" />
		
	<puts text="Test: Calendar_0702.xml finished." />
</soda>