<soda>
	<puts text="Test: Reports_7736.xml starting." />
	<!--
			Action
		1. Log in as admin.
		2. Click "Reports" tab on navigation bar.
		3. Click "Create Report" link from navigation Shortcuts.
		4. Click "Row and Column Report".
		5. Click "Lead" module.
		6. Click "Lead" in "Related Modules" panel.
		
			Expectation:
		1.  Reports To not present
	-->
	
	<link id="moduleTab_Reports" />
	
	<link text="Create Report" />
	
	<wait timeout="2" />
	<div id="report_type_div">
		<image name="rowsColsImg" jscriptevent="onclick" click="true" />
	</div>
	
	<table id="Leads">
		<link text="Leads" />
	</table>
	
	<button value="Next &gt;" />
	
	<div id="module_fields_panel">
		<div text="Reports To" exists="false" />
	</div>
	
	<puts text="Test: Reports_7736.xml finished." />
</soda>	
