<soda>
	<!-- delete report -->
	<puts text="Test: Reports_8047.xml started." />

	<script file="{@global.scriptsdir}/modules/lib/Report_Create_Lib.xml" />

	<!--goto Report module -->
	<link id="moduleTab_Reports" />
	<link id="Basic Search" required="false" />
	<wait timeout="5" />
	<form class="search_form">
		<button name="clear" />
		<textfield name="name_basic" set="{@pre_report_name}" />
		<button value="Search" />
	</form>
	<wait timeout="5" />
	<table class="list view">
		<link text="{@pre_report_name}" />
	</table>
	<button id="deleteReportButton" alert="true" />
	<wait timeout="5" />
	<link id="moduleTab_Reports" />
	<link id="Basic Search" required="false" />
	<wait timeout="5" />
	<form class="search_form">
		<button name="clear" />
		<button value="Search" />
	</form>
	<table class="list view">
		<span class="pageNumbers" assert="(1 - 20 of 44)" />
	</table>

	<puts text="Test: Reports_8047.xml finished." />
</soda>