<soda>
	<puts text="Test: Opportunity_2333.xml staring." />
	<!-- Tests "cancel" of a note object on Opps subpanel -->
	<csv file="{@global.scriptsdir}/csvs/opportunity/Opportunity_2333.csv" var="Opportunity_2333">

		<link id="moduleTab_Opportunities" />
		<browser assert="Opportunities" />
		<button id="search_form_clear" />
		<form id="search_form">
			<textfield id="name_basic" set="{@pre_opp_name}" />
		</form>
		<button id="search_form_submit" />

		<table class="list view">
			<browser assert="{@pre_opp_name}" />
			<link text="{@pre_opp_name}" />
		</table>

		<table id="detailpanel_1">
			<browser assert="{@pre_opp_name}" />
		</table>

		<button value="  Create Note or Attachment  " />

		<!--teams select -->
		<wait timeout="10" condition="true">
   			<button name="teamSelect" />
		</wait>
		<button name="teamSelect" />
		<wait timeout="10" condition="true">
   			<button name="teamSelect" />
		</wait>
		<browser assert="Team Search" />
		<textfield name="name" set="{@Opportunity_2333.team_name}" />
		<button value="Search" />
		<link text="{@Opportunity_2333.team_name}" />

		<textfield id="name" set="{@Opportunity_2333.note_name} {@user.name} {@stamp}" />
		<textarea id="description" set="{@Opportunity_2333.note_description}" />
		<button id="Notes_subpanel_cancel_button" alert="true" />

		<div id="list_subpanel_history">
			<table class="list view">
				<link text="{@Opportunity_2333.note_name} {@user.name} {@stamp}" exist="false" />
			</table>
		</div>

	</csv>
	<!--Run Delete XML -->
	<script file="{@global.scriptsdir}/modules/lib/Note_Delete_Lib.xml" />
	<puts text="Test: Opportunity_2333.xml finished." />
</soda>
