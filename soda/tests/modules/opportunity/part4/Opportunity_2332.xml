<soda>
	<puts text="Test: Opportunity_2332.xml starting." />

	<csv file="{@global.scriptsdir}/csvs/opportunity/Opportunity_2332.csv" var="Opportunity_2332">

		<link id="moduleTab_Opportunities" />
		<browser assert="Opportunities" />
		<button id="search_form_clear" />
		<form id="search_form">
			<textfield id="name_basic" set="{@pre_opp_name}" />
		</form>
		<button id="search_form_submit" />

		<table class="list view">
			<browser assert="{@pre_opp_name}" />
			<link text="{@pre_opp_name}" />
		</table>

		<table id="detailpanel_1">
			<browser assert="{@pre_opp_name}" />
		</table>


		<button value="  Create Note or Attachment  " />

		<!--teams select -->

		<table id="form_SubpanelQuickCreate_Notes_team_name_table">
			<button value="Select" />
		</table>

		<browser assert="Team Search" />
		<textfield name="name" set="{@Opportunity_2332.team_name}" />
		<button value="Search" />
		<link text="{@Opportunity_2332.team_name}" />

		<textfield id="name" set="{@Opportunity_2332.note_name} {@user.name} {@stamp}" />
		<textarea id="description" set="{@Opportunity_2332.note_description}" />
		<button id="Notes_subpanel_save_button" />


		<div id="list_subpanel_history">
			<table class="list view">
				<link text="{@Opportunity_2332.note_name} {@user.name} {@stamp}" click="true" />
			</table>
		</div>

		<browser assert="{@Opportunity_2332.note_name} {@user.name} {@stamp}" />

	</csv>
	<!--Run Delete XML -->
	<script file="{@global.scriptsdir}/modules/lib/Call_Delete_Lib.xml" />
	<puts text="Test: Opportunity_2332.xml finished." />
</soda>
