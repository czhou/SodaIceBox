
<soda>
	<!-- Quote_Activities_Subpanel_Full_form_schedule_Call. -->
	<puts text="Test: Quote_8553.xml starting" />
	<csv file="{@global.scriptsdir}/csvs/quote/Quote_8553.csv" var="Quote_8553">
		<link href="index.php?module=Quotes&amp;action=index" />
		<textfield id="name_basic" set="{@pre_Quote_name}" />
		<button id="search_form_submit" />
		<table class="list view">
			<browser assert="{@pre_Quote_name}" />
			<link text="{@pre_Quote_name}" click="true" />
		</table>
		<table id="detailpanel_1">
			<browser assert="{@pre_Quote_name}" />
		</table>
		<div id="list_subpanel_activities">
			<table class="list view">
				<button id="Activities_logcall_button" />
				<wait timeout="3" />
			</table>
		</div>
		<form id="form_SubpanelQuickCreate_Calls">
			<button id="Calls_subpanel_full_form_button" />
		</form>
		<timestamp />
		<form id="EditView">
			<textfield id="name" set="{@Quote_8553.call_name} {@user.name} {@stamp}" />
			<textfield id="date_start_date" set="{@Quote_8553.call_date_start_date}" />
			<select id="date_start_hours" set="{@Quote_8553.call_date_start_hours}" />
			<select id="date_start_minutes" set="{@Quote_8553.call_date_start_minutes}" />

			<textfield name="duration_hours" set="{@Quote_8553.call_duration_hours}" />
			<select name="duration_minutes" set="{@Quote_8553.call_duration_minutes}" />

			<select id="direction" set="{@Quote_8553.call_direction}" />
			<select id="status" set="{@Quote_8553.call_status}" />

			<checkbox name="reminder_checked" set="{@Quote_8553.call_reminder_checked}" />
			<textfield id="description" set="{@Quote_8553.call_description}" />
		</form>

		<form id="EditView">
			<button value="Save" />
		</form>
		<div id="list_subpanel_activities">
			<table class="list view">
				<link text="{@Quote_8553.call_name} {@user.name} {@stamp}" click="false" />
			</table>
		</div>
	</csv>

	<!--Run Delete XML -->
	<script file="{@global.scriptsdir}/modules/lib/Call_Delete_Lib.xml" />

	<puts text="---Quote_8553.xml---Finished!" />
</soda>
