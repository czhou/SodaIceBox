
<soda>
	<!--Create contact_Verify that creating a contact can be canceled-->
	<puts text="Conacts_7050 starting." />
	<timestamp />
	<link id="moduleTab_Contacts" />
	<csv file="{@global.scriptsdir}/modules/lib/Contact_Create_Lib.csv" var="contact_record">
		<link text="Create Contact" />
		<browser assert="Contact Overview" />

		<!--fill the fields and "Save"-->
		<form id="EditView">
			<textfield name="first_name"
				set="{@contact_record.first_name} {@user.name} {@stamp}" />
			<textfield id="last_name" set="{@contact_record.last_name}" />
			<wait timeout="2" />
			<button id="CANCEL_HEADER" alert="true"/>
		</form>

		<textfield id="search_name_basic" set="{@contact_record.first_name} {@user.name} {@stamp} {@contact_record.last_name}"/>
		<button id="search_form_clear"/>
		<button id="search_form_submit"/>
		<browser assertnot="{@contact_record.first_name} {@user.name} {@stamp} {@contact_record.last_name}" />
	</csv>

	

	<puts text="Contacts_7050.xml finished." />
</soda>

