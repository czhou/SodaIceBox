
<soda>
	<!--
		Search contact_Verify that contacts with special characters can be searched. (For example, with "@$*&" characters, etc)
	-->
	<puts text="Test: Contacts_10179.xml starting" />

	<!--Create a records and save its counter-->
	<script file="{@global.scriptsdir}/modules/lib/Contact_Create_Lib.xml"/>
	<csv file="{@global.scriptsdir}/modules/lib/Contact_Create_Lib.csv" var="Contacts_10179"/>

	<link id="moduleTab_Contacts" />
	
	<!--Goto advanced search view-->
	<link id="advanced_search_link" required="false"/>

	<!--Search by any phone and with blank space before the number-->
	<textfield id="phone_advanced" set=" {@Contacts_10179.phone_work} "/>
	<button id="search_form_submit"/>
	
	<!--Check searching result-->
	<table class="list view">
		<link text="{@pre_contact_first_name} {@pre_contact_last_name}" click="false" exists="true"/>
	</table>

	<!--Clean the record-->
	<link id="basic_search_link" required="false"/>
	<script file="{@global.scriptsdir}/modules/lib/Contact_Delete_Lib.xml"/>
	<puts text="Test: Contacts_10179.xml finished" />
</soda>
