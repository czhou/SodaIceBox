<soda>
	<!-- Edit Note_Verify that modification of note for call/meeting can be canceled. -->
	<puts text="Test: Calendar_0723.xml starting" />

	<script file="{@global.scriptsdir}/modules/lib/Meeting_Create_Lib.xml" />

	<link id="moduleTab_Meetings" />
	<div class="moduleTitle">
		<link href="index.php?module=Meetings&amp;action=index" click="false" />
		<browser assert="Search" />
	</div>
	<link text="View Meetings" />

	<textfield id="name_basic" set="{@pre_meeting_name}" />
	<button id="search_form_submit" />
	<wait timeout="3" />

	<link text="{@pre_meeting_name}" />
	<wait timeout="3" />
	<div class="moduleTitle">
		<link href="index.php?module=Meetings&amp;action=index" click="false" />
		<browser assert="{@pre_meeting_name}" />
	</div>
	<csv file="{@global.scriptsdir}/csvs/calendar/Calendar_0722.csv" var="calendar_0723" />
	<div id="list_subpanel_history">
		<button name="History_createnoteorattachment_button" />
	</div>
	<div id="form_SubpanelQuickCreate_Notes_tabs">
		<textfield id="name" set="{@calendar_0723.subject}" />
		<textfield id="description" set="{@calendar_0723.description}" />
	</div>

	<form id="form_SubpanelQuickCreate_Notes">
		<button id="Notes_subpanel_save_button" />
	</form>
	<wait timeout="5" />

	<div id="list_subpanel_history">
		<link class="listViewTdToolsS1" index="1" />
	</div>
	<wait timeout="3" />
	<textfield id="name" set="{@calendar_0723.subject2}" />
	<textfield id="description" set="{@calendar_0723.description2}" />

	<button value="Cancel" alert="true" />
	<wait timeout="3" />
	
	<div id="list_subpanel_history">
		<link text="{@calendar_0723.subject2}" exist="false" />
	</div>

	<script file="{@global.scriptsdir}/modules/lib/Meeting_Delete_Lib.xml" />

	<puts text="Test: Calendar_0723.xml finished" />
</soda>