
<soda>
	<!-- Shared Calendar User Selection Does Not Filter Out Inactive Users -->
	<puts text="Test: Calendar_10966.xml starting"/>

	<!-- Create user -->
	<script file="{@global.scriptsdir}/modules/lib/User_Create_Lib.xml" />
	
	<!-- Set newly created user to inactive -->
	<button value="Edit" />
	<select name="status" set="Inactive" />
	<button value="Save" />
	<button text="No" />
	
	<!-- Navigate to Calendar module's shared calendar -->
	<link id="moduleTab_Calendar" />
	<button value="/Shared/" />
	<wait timeout="3" />
	<link text="/Edit/" />
	<wait timeout="3" />
	
	<!-- Verify that inactive user is not in the filters -->
	<select name="team_id" set="1" /> <!-- team Global -->
	<select id="shared_ids" noninclude="{@pre_user_name}" />

	<!-- Delete created user -->
	<script file="{@global.scriptsdir}/modules/lib/User_Delete_Lib.xml" />
	
	<puts text="Test: Calendar_10966.xml finished"/>
</soda>
