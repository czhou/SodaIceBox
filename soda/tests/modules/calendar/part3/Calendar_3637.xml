
<soda>
	<!--
		Action:
			1. Login to system as a valid user.
			2. Click "Calendar" navigation tab.
			3. Click "Meetings" link on Navigation shortcuts.
			4. Click any meeting subject link in "Meeting List" view.
			5. Click "Delete" button on "Meeting Detail View" page.
			6. Click "OK" button on confirm dialog box.
		Expectation:
		`	6. The deleted meeting is not displayed in the meeting list view.
	-->
	<puts text="Test: Calendar_3637.xml starting." />
	
	<script file="{@global.scriptsdir}/modules/lib/Meeting_Create_Lib.xml" />
	<var var="meeting_name" set="{@pre_meeting_name}" />
	
	<button value="Delete" alert="true" />
	<link id="moduleTab_Meetings" />
	
	<table class="list view">
		<browser assertnot="{@meeting_name}" />
	</table>
	
	<puts text="Test: Calendar_3637.xml finished." />
</soda>
