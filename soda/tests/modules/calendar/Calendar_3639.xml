<soda>
	<!--
		Action:
			1. Login to system as a valid user.
			2. Click "Calendar" navigation tab.
			3. Click "Meetings" link on Navigation shortcuts.
			2,3.  This must now be accessed directly via moduleTab_Meetings
			4. Click any meeting subject in "Meeting List" view.
			5. Click "Edit" button on "Meeting Detail View" page.
			6. Update meeting information on "Edit Meeting" page.
			7. Click "Save" button on "Edit Meeting" page.
		Expectation:
		`	7. Updated meeting information is displayed on "Meeting Detail View" page
	-->
	<puts text="Test: Calendar_3639.xml starting." />
	
	<script file="{@global.scriptsdir}/modules/lib/Meeting_Create_Lib.xml" />
	<var var="meeting_name" set="{@pre_meeting_name}" />
	
	<link id="moduleTab_Meetings" />
	
	<table class="list view">
		<link text="{@meeting_name}" />
	</table>
	
	<button id="edit_button" />
	
	<textarea id="name" set="{@meeting_name} Modified" />
	<select id="status" set="Held" />
	<textfield id="date_start_date" set="12/21/2012" />
	<select id="date_start_hours" set="01" />
	<select id="date_start_minutes" set="45" />
	<textfield id="location" set="White Room" />
	<textarea id="description" set= "Amended Description" />
	
	<button value="Save" />
	
	<wait timeout="6" />
	
	<div id="LBL_MEETING_INFORMATION">
		<browser assert="{@meeting_name} Modified" />
		<browser assert="Held" />
		<browser assert="12/21/2012 01:45" />
		<browser assert="White Room" />
		<browser assert="Amended Description" />
	</div>
	
	<script file="{@global.scriptsdir}/modules/lib/Meeting_Delete_Lib.xml" />
	
	<puts text="Test: Calendar_3639.xml finished." />
</soda>