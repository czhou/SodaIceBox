
<soda>        
	<!--
		selecting an Assigned User in the create Account pop-up
	-->
	<puts text="Test: Opportunity_11765.xml started." />
	<timestamp />
	<link id="moduleTab_Opportunities" />
	<link text="Create Opportunity" />

	<csv file="{@global.scriptsdir}/modules/lib/Opp_Create_Lib.csv" var="opp_11756">
		<textfield id="name" set="{@opp_11756.name} {@user.name} {@stamp}" /> 
		<textfield id="amount" set="{@opp_11756.amount}" /> 
		<textfield id="probability" set="{@opp_11756.probability}" /> 
		<textfield id="date_closed" set="{@opp_11756.date_closed}" /> 
		<textfield id="next_step" set="{@opp_11756.next_step}" />
		<textfield id="description" set="{@opp_11756.description}" />
		<select id="opportunity_type" set="{@opp_11756.opportunity_type}" />
		<select id="lead_source" set="{@opp_11756.lead_source}" />
		<select name="currency_id" set="{@opp_11756.currency_id}" />
		<select id="sales_stage" set="{@opp_11756.sales_stage}" />

		<!--Account Select-->
		<button id="btn_account_name" />
		<browser assert="Account Search" />

		<!--Create a new account -->
		<button value="Create Account"/>
		<textfield id="name" set="Opp_11756_Account_name"/>

		<!--Select a assigned user-->
		<button id="btn_assigned_user_name"/>
		<link text="Administrator"/>
		<button id="Accounts_popupcreate_save_button" />
		<link text="Opp_11756_Account_name"/>

		<!--Save the new account-->
		<button id="SAVE_HEADER"/>
	</csv>

	<puts text="Test: Opportunity_11756.xml finished." />
</soda>
