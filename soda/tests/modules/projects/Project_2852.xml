
<soda>

	<puts text="Test Case 2852: Create Call -- Project Management_Verify that scheduling a call can be canceled for Project."/>

	<!--Search out the project record created in lib-->
        <link id="moduleTab_Project"/>
	<div class="moduleTitle">
		<link href="index.php?module=Project&action=index" click="false"/>
	</div>

	<csv file="{@global.scriptsdir}/csvs/projects/Project_2851.csv" var="Project_2851">
		<textfield id="name_basic" set="{@pre_project_name}"/>
		<button id="search_form_submit"/>
		<link text="{@pre_project_name}"/>
		<browser assert="Resources"/>
		<!--Create Call record-->
		<button id="Activities_logcall_button"/>
		<textfield id="name" set="{@Project_2851.call_name}"/>
		
		<button id="Calls_subpanel_cancel_button" alert="true"/>
		<wait timeout="3"/>	
		
		<link text="{@Project_2851.call_name}" exist="false" click="false"/>
		
	</csv>

        <puts text="Test Case 2852 is finished!"/>
</soda>
