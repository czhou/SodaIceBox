
<soda>
	
	<puts text="Test: PM_9429 set maximum length of password to invalid value" />

	<div id="globalLinks">
		<link text="Admin" />
	</div>
	<csv file="{@global.scriptsdir}/csvs/passwordmanagement/PM_9429.csv" var="pm9429">	
	
		<link text="Password Management" />
		<table id="passRequirementId">
			<browser assert="Password Requirements " />
			<textfield name="passwordsetting_minpwdlength" set="" />
			<checkbox id="passwordsetting_oneupper" set="false" />
			<checkbox id="passwordsetting_onenumber" set="false" />
			<checkbox id="passwordsetting_onelower" set="false" />
			<textfield name="passwordsetting_maxpwdlength" set="{@pm9429.max_len}" />
		</table>
		<button id="btn_save" />
		<browser assert="Invalid Value: Maximum Length" />
		<button name="cancel" />
	</csv>

	<puts text="Test: PM_9429.xml finished." />
</soda>
