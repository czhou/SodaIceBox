
<soda>
	
	<puts text="Test: PM_9417 set minimum length of password to invalid value" />

	<div id="globalLinks">
		<link text="Admin" />
	</div>
	<csv file="{@global.scriptsdir}/csvs/passwordmanagement/PM_9417.csv" var="pm9417">	
	
		<link text="Password Management" />
		<table id="passRequirementId">
			<browser assert="Password Requirements " />
			<textfield name="passwordsetting_minpwdlength" set="{@pm9417.min_len}" />
		</table>
		<button id="btn_save" />
		<browser assert="Invalid Value: Minimum Length" />
		<button name="cancel" />
	</csv>

	<puts text="Test: PM_9417.xml finished." />
</soda>
