
<soda>
	
	<puts text="Test: PM_9430 set maximum length is less than requirements " />

	<div id="globalLinks">
		<link text="Admin" />
	</div>
	<csv file="{@global.scriptsdir}/csvs/passwordmanagement/PM_9430.csv" var="pm9430">	
	
		<link text="Password Management" />
		<table id="passRequirementId">
			<browser assert="Password Requirements " />
			<textfield name="passwordsetting_minpwdlength" set="" />
			<checkbox id="passwordsetting_oneupper" set="true" />
			<checkbox id="passwordsetting_onenumber" click="true" />
			<checkbox id="passwordsetting_onelower" set="true" />
			<textfield name="passwordsetting_maxpwdlength" set="{@pm9430.max_len}" />
		</table>
		<button id="btn_save" />
		<browser assert="Maximum Length should be 2 or greater" />
		<button name="cancel" />
	</csv>

	<puts text="Test: PM_9430.xml finished." />
</soda>
