
<soda>
	
	<puts text="Test: PM_9412 set minimum length of password" />

	<div id="globalLinks">
		<link text="Admin" />
	</div>
	<csv file="{@global.scriptsdir}/csvs/passwordmanagement/PM_9412.csv" var="pm9412">	
	
		<link text="Password Management" />
		<table id="passRequirementId">
			<browser assert="Password Requirements " />
			<textfield name="passwordsetting_minpwdlength" set="{@pm9412.min_len}" />
		</table>
		<button id="btn_save" />
		<div id="welcome">
			<link text="Administrator" />
		</div>
		<form id="EditView">
			<browser assert="User Profile" />
			<link text="Password" />
			<browser assert="Minimum Length ={@pm9412.min_len}" />
			<button value="Cancel" />
		</form>
	</csv>
	
	<div id="globalLinks">
		<link text="Admin" />
	</div>
	<link text="Password Management" />
	<table id="passRequirementId">
		<browser assert="Password Requirements " />
		<textfield name="passwordsetting_minpwdlength" set="3" />
	</table>
	<button id="btn_save" />

	<puts text="Test: PM_9412.xml finished." />
</soda>
