
<soda>
	
	<puts text="Test: PM_9419 set maximum length of password" />

	<div id="globalLinks">
		<link text="Admin" />
	</div>
	<csv file="{@global.scriptsdir}/csvs/passwordmanagement/PM_9419.csv" var="pm9419">	
	
		<link text="Password Management" />
		<table id="passRequirementId">
			<browser assert="Password Requirements " />
			<textfield name="passwordsetting_maxpwdlength" set="{@pm9419.max_len}" />
		</table>
		<button id="btn_save" />
		<div id="welcome">
			<link text="Administrator" />
		</div>
		<form id="EditView">
			<browser assert="User Profile" />
			<link text="Password" />
			<browser assert="Minimum Length =3 and Maximum Length ={@pm9419.max_len}" />
			<button value="Cancel" />
		</form>
	</csv>
	
	<div id="globalLinks">
		<link text="Admin" />
	</div>
	<link text="Password Management" />
	<table id="passRequirementId">
		<browser assert="Password Requirements " />
		<textfield name="passwordsetting_maxpwdlength" set="" />
	</table>
	<button id="btn_save" />

	<puts text="Test: PM_9419.xml finished." />
</soda>
