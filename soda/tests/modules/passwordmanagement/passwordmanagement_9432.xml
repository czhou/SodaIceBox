
<soda>
	<!-- show password rules -->
	<puts text="Test: passwordmanagement_9432.xml starting." />

	<div id="globalLinks">
		<link text="Admin" />
	</div>
	<csv file="{@global.scriptsdir}/csvs/passwordmanagement/PM_9432.csv" var="pm9432">	
	
		<link text="Password Management" />
		<table id="passRequirementId">
			<browser assert="Password Requirements " />
			<textfield name="passwordsetting_maxpwdlength" set="{@pm9432.max_len}" />
		</table>
		<button id="btn_save" />
		<div id="welcome">
			<link text="Administrator" />
		</div>
		<form id="EditView">
			<browser assert="User Profile" />
			<link text="Password" />
			<table class="x-sqs-list">
				<div id="1upcase" class="bad" assert="" />
				<div id="lengths" class="bad" assert="" />
				
				<browser assert="Must contain one upper case letter (A-Z)" />
				<browser assert="Minimum Length =3 and Maximum Length ={@pm9432.max_len}" />
			</table>
			<button value="Cancel" />
		</form>
	</csv>
	
	<div id="globalLinks">
		<link text="Admin" />
	</div>
	<link text="Password Management" />
	<table id="passRequirementId">
		<browser assert="Password Requirements " />
		<textfield name="passwordsetting_maxpwdlength" set="" />
	</table>
	<button id="btn_save" />

	<puts text="Test: passwordmanagement_9432.xml finished." />
</soda>
