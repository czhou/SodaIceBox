
<soda>
	
	<puts text="Test: PM_9414 set minimum length of password to negative number" />

	<div id="globalLinks">
		<link text="Admin" />
	</div>
	<csv file="{@global.scriptsdir}/csvs/passwordmanagement/PM_9414.csv" var="pm9414">	
	
		<link text="Password Management" />
		<table id="passRequirementId">
			<browser assert="Password Requirements " />
			<textfield name="passwordsetting_minpwdlength" set="{@pm9414.min_len}" />
		</table>
		<button id="btn_save" />
		<browser assert="Minimum Length should be 0 or greater" />
		<button name="cancel" />
	</csv>

	<puts text="Test: PM_9414.xml finished." />
</soda>
