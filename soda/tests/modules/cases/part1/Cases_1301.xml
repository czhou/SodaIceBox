
<soda>
	<!-- Create Archived Email: Verify that archived email cannot be created for case in "History" sub-panel when entering non-existing data (such as entering a non-existing team in "Team" field). -->
	<puts text="Test: Cases_1301.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/cases/CasesAssertText.csv" var="cases" />
	<timestamp />

	<!-- Create a case and go to its DetailView. -->
	<script file="{@global.scriptsdir}/modules/lib/Case_Create_Lib.xml" />
	<div class="moduleTitle">
		<link href="index.php?module=Cases&amp;action=index" click="false" />
	</div>

	<!-- Create an archived email. -->
	<csv file="{@global.scriptsdir}/csvs/cases/Case_1301.csv" var="Case_1301">
		<button id="ArchiveEmail_button" />

		<textfield name="date_start" set="{@Case_1301.email_date}" />
		<textfield name="time_start" set="{@Case_1301.email_time}" />
		<textfield id="EditView_team_name_collection_0" click="true" />
		<textfield id="EditView_team_name_collection_0" set="{@Case_1301.email_team}" />
		<button id="SAVE_HEADER" />
		
		<!-- Assert error messages. -->
		<browser assert="No Primary Team specified" />

		<button value="  Cancel  " alert="true" />
	</csv>

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Case_Delete_Lib.xml" />

	<puts text="Test: Cases_1301.xml finished." />
</soda>
