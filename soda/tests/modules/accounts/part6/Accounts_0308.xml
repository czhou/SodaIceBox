
<soda>
	<!-- Account Detail - History sub-panel-Duplicate_Verify that duplicating an activity record is canceled. -->
	<puts text="Test: Accounts_0308.xml starting."/>

	<script file="{@global.scriptsdir}/modules/lib/Account_Create_Lib.xml" />

	<!--Create Meeting record-->
	<div id="LBL_ACCOUNT_INFORMATION">
		<browser assert="{@pre_account_name}"/>
	</div>

	<button id="Activities_schedulemeeting_button"/>
	<wait timeout="7" />
	<textfield id="name" set="Account meeting record"/>
	<select id="status" set="Held"/>
	<button id="Meetings_subpanel_save_button"/>
	<wait timeout="12"/>

	<div id="list_subpanel_history">
		<browser assert="Account meeting record" />
		<link text="Account meeting record"/>
	</div>
	<button id="duplicate_button"/>
	<button id="CANCEL" alert="true"/>

	<link id="moduleTab_Meetings"/>
	<button id="search_form_clear"/>
	<textfield id="name_basic" set="Account meeting record"/>
	<button id="search_form_submit"/>
	<span class="pageNumbers" assert="(1 - 1 of 1)"/>
	
	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Meeting_Delete_Lib.xml"/>

	<puts text="Test: Accounts_0308.xml finished." />
</soda>
