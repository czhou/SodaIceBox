
<soda>
	<!-- Account Detail - Member Organizations sub-panel - Create_Verify that error message 'Missing required field: Name' is displayed when creating related new 'Member Organizations' with mandatory fields blank. -->
	<puts text="Test: Accounts_0234.xml starting." />

	<script file="{@global.scriptsdir}/modules/lib/Account_Create_Lib.xml" />

	<link id="moduleTab_Accounts" />
	<div class="moduleTitle">
		<link href="index.php?module=Accounts&amp;action=index" click="false" />
	</div>

	<table class="list view">
		<link text="{@pre_account_name}" /> 
	</table>

	<!--Create Member record-->
	<div id="LBL_ACCOUNT_INFORMATION">
		<browser assert="{@pre_account_name}" />
	</div>

	<button id="member_accounts_create_button" />
	<wait timeout="3" />
	<textfield id="name" set="" />
	<button id="Accounts_subpanel_save_button" />
	<wait timeout="3" />

	<browser assert="Missing required field: Name" />
	<link id="moduleTab_Accounts" alert="true" />

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />

	<puts text="Test: Accounts_0234.xml finished." />
</soda>
