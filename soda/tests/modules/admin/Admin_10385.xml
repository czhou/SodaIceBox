
<soda>
<!-- Delete private team from list view with the corresponding user still exists -->
    <puts text="Test: Admin_10385.xml starting" />
    <!--goto Admin-->
    <div id="globalLinks">
        <link text="Admin" />
    </div>
	<csv file="{@global.scriptsdir}/modules/lib/SugarInit_Lib.csv" var="SugarInit">
		<!--click "Team Management"-->
		<link href="./index.php?module=Teams&amp;action=index" />
		<browser assert="Teams" />
		<!--try to delete team from list view-->
		<button value="Clear" />
		<textfield name="name_basic" set="{@SugarInit.last_name}" />
		<button value="Search" />
		<table class="list view">	
			<checkbox name="massall" click="true" />				
		</table>
		<link id="actions_link" />
		<link text="Delete" alert="true" />
		<browser assert="This private team [{@SugarInit.last_name}] cannot be deleted until the user [{@SugarInit.last_name}] is deleted." />
		
		<!--verify team not delelted-->
		<link text="Teams" />
		<browser assert="Teams" />
		
		<button value="Clear" />
		<textfield name="name_basic" set="{@SugarInit.last_name}" />
		<button value="Search" />
		<table class="list view">
			<link text="{@SugarInit.last_name}" click="false" />
		</table>
	</csv>

    <puts text="Test: Admin_10385.xml finished" />
</soda>
