<soda>
	<!--
		a) Login to Pro or Ent as admin user
		b) Go to Admin | Connectors | Set Connector Properties | Twitter tab
		
		Results:
		b) Make sure Twitter tab is shown ".
	-->
	<puts text="Text: Reports_9894.xml Starting." />

	<browser url="{@global.url}?module=Connectors&amp;action=ModifyProperties" />
	<browser assert="Twitter" />
	
	<!-- navigate to 5th / Twitter tab... -->
	<div id="container">
		<div index="1">
			<li index="5">
				<link index="1" />
			</li>
		</div>
	</div>
	<div class="yui-content">
		<browser assert="Consumer Key:" />
		<browser assert="Consumer Secret:" />
		<textfield id="ext_rest_twitter_oauth_consumer_key"	assert="" />
		<textfield id="ext_rest_twitter_oauth_consumer_secret" assert="" />
	</div>
	<puts text="Test: Reports_9892.xml finished." />
</soda>