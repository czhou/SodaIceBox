<soda>
	<!--
		a)    Login SugarCRM as a valid user
		b)    Go to Admin page
		c)    Click "Display Module Tabs and Subpanels" link
		d)    Check description is as following:
		"Select Module Tabs and Subpanels"
		"Drag and Drop the names of the modules below to set tabs or
		subpanels to be displayed or to be hidden"
	-->
	<puts text="Test: Admin_10287.xml starting." />

	<link text="Admin" />

	<link text="Display Module Tabs and Subpanels" />

	<browser assert="Display Module Tabs and Subpanels" />
	<browser assert=
	"Drag and Drop the names of the modules below to set tabs or subpanels to be displayed or to be hidden." />	
	
	<puts text="Test: Admin_10287.xml finished." />
</soda>
