<soda>
	<puts text="Test: Admin_9267.xml starting." />

	<link id="admin_link" />

	<link text="Connectors" />

	<!--Nav to Enable Connectors Configuration-->
	<image name="enableImage" click="true" />
	<wait timeout="3" />
	<div id="ext_rest_linkedin:enabled_div">
		<browser assert="Accounts" />
		<browser assert="Contacts" />
		<browser assert="Leads" />
		<browser assert="Targets" />
	</div>
	<browser url="{@global.url}?module=Connectors&amp;action=SaveModifyDisplay&amp;modify=true&amp;display_values=ext_rest_linkedin:Accounts,ext_rest_linkedin:Contacts&amp;display_sources=ext_rest_linkedin&amp;button=Save" />
	<!--Nav to Enable Connectors Configuration-->
	<image name="enableImage" click="true" />
	<wait timeout="3" />
	<div id="ext_rest_linkedin:enabled_div">
		<browser assert="Accounts" />
		<browser assert="Contacts" />
	</div>

	<!-- Clean Up: reset enabled connectors to the original configuration. -->
	<browser url="{@global.url}?module=Connectors&amp;action=SaveModifyDisplay&amp;modify=true&amp;display_values=ext_rest_linkedin:Accounts,ext_rest_linkedin:Contacts,ext_rest_linkedin:Leads,ext_rest_linkedin:Prospects&amp;display_sources=ext_rest_linkedin&amp;button=Save" />
	
	<puts text="Test: Admin_9267.xml finished." />
</soda>
