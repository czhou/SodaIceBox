
<soda>

	<!--
		Verify  subpanel name sync with updating module  name
	-->
	<puts text="Test: Admin_11595.xml starting." />

	<!--Go to Admin page-->
	<link text="Admin" />

	<!--Go to Rename module name page-->
	<link text="Rename Modules" />
	
	<!--Update one module singular name, such as "Contact"-->
	<span id="slot1b">
		<image index="1" click="true"/>
	</span>

	<!--Update string "Contacts" as "Cons" and save-->
	<textfield id="slot1_text" set="Cons" />
	<textfield id="slot1_text" jscriptevent="onchange" />
	<wait timeout="2"/>
	<table id="contentTable">
		<button name="Save" value="Save"/>
	</table>

	<!--Create a new call-->
	<script file="{@global.scriptsdir}/modules/lib/Call_Create_Lib.xml"/>
	<div id="subpanel_title_contacts">
		<browser assert="Cons" />
	</div>

	<!-- Check module name show "Cons" -->
	<link text="Cons" click="false" exists="true"/>

	<!-- Go to Admin page -->
	<link text="Admin"/>

	<!-- Go to Rename module name page -->
	<link text="Rename Modules"/>
	
	<!-- Update one module back to default values -->
	<span id="slot1b">
		<image index="1" click="true"/>
	</span>

	<!-- Update string "Contacts" as "Cons" and save -->
	<textfield id="slot1_text" set="Contacts" />
	<textfield id="slot1_text" jscriptevent="onchange" />
	<wait timeout="2" />
	<table id="contentTable">
		<button name="Save" value="Save" />
	</table>

	<script file="{@global.scriptsdir}/modules/lib/Call_Delete_Lib.xml" />

	<puts text="Test: Admin_11595.xml finished" />
</soda>
