
<soda>

	<!--
		Verify  module name in report wizard sync with updating module  name
	-->

	<puts text="Test: Admin_11601.xml starting" />

	<!--Create a new contact -->
	<script file="{@global.scriptsdir}/modules/lib/Case_Create_Lib.xml"/>

	<!--Go to Admin page-->
	<link text="Admin"/>

	<!--Go to Rename module name page-->
	<link text="Rename Modules"/>
	
	<!--Update one module singular name, such as "Cases"-->
	<span id="slot4b">
		<image index="1"/>
	</span>

	<!--Update string "Case" as "Ca" and save-->
	<textfield id="slot4_stext" set="Ca"/>
	<textfield id="slot4_text" set="Cas"/>
	<table id="contentTable">
		<button name="Save" value="Save"/>
	</table>

	<!--Goto Reports module and select create a new Summaiation report-->
	<link id="moduleTab_Reports"/>
	<link text="Create Report"/>
	<image name="summationImg" click="true"/>
	<wait timeout="5"/>

	<table id="Cases">
		<link text="Cas" alert="true"/>
	</table>



	<!--Set back to default module name-->
	<!--Go to Admin page-->
	<link text="Admin"/>

	<!--Go to Rename module name page-->
	<link text="Rename Modules"/>
	
	<!--Update one module singular name, such as "Cases"-->
	<span id="slot4b">
		<image index="1"/>
	</span>

	<!--Update string "Ca" as "Case" and "Cas" as "Cases" then save-->
	<textfield id="slot4_stext" set="Case"/>
	<textfield id="slot4_text" set="Cases"/>
	<table id="contentTable">
		<button name="Save" value="Save"/>
	</table>

	<script file="{@global.scriptsdir}/modules/lib/Case_Delete_Lib.xml"/>
	<puts text="Test: Admin_11601.xml finished" />
</soda>
