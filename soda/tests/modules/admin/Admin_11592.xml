
<soda>
	<!--
		Verify user can modify navigation bar name by renaming module  name
	-->
	<puts text="Test: Admin_11592.xml starting" />

	<!--Go to Admin page-->
	<link text="Admin"/>

	<!--Go to Rename module name page-->
	<link text="Rename Modules"/>
	
	<!--Update one module singular name, such as "Contact"-->
	<span id="slot1b">
		<image index="1" click="true"/>
	</span>

	<!--Update string "Contacts" as "Cons" and save-->
	<textfield id="slot1_text" set="Cons"/>
	<textfield id="slot1_text" jscriptevent="onchange"/>
	<table id="contentTable">
		<button name="Save" value="Save"/>
	</table>

	<!--Go to Contacts module-->
	<link  id="moduleTab_Contacts"/>

	<!-- Check module name show "Cons"-->
	<link text="Cons" click="false" exists="true"/>

	<!-- Check module name show "Cons"-->
	<link text="View Cons" click="false" exists="true"/>
	<link text="Import Cons" click="false" exists="true"/>

	<!--Set back to default module name-->
	<!--Go to Admin page-->
	<link text="Admin"/>

	<!--Go to Rename module name page-->
	<link text="Rename Modules"/>
	
	<!--Update one module singular name, such as "Contact"-->
	<span id="slot1b">
		<image index="1" click="true"/>
	</span>

	<!--Update string "Cons" as "Contacts" and save-->
	<textfield id="slot1_text" set="Contacts"/>
	<textfield id="slot1_text" jscriptevent="onchange"/>
	<table id="contentTable">
		<button name="Save" value="Save"/>
	</table>

	<puts text="Test: Admin_11592.xml finished" />
</soda>
