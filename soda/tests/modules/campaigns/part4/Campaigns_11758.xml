
<soda>
	<!-- Delete the campaign-account relationship during the campaign deleting -->
	<puts text="Test: Campaigns_11758.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/campaigns/CampaignAssertText.csv" var="campaigns" />

	<!-- Create one campaign records. -->
	<script file="{@global.scriptsdir}/modules/lib/Campaign_Create_Lib.xml" />

	<link id="moduleTab_Accounts" />
	<link text="Create Account" />
	<form id="EditView">
		<textfield id="name" set="Account_Campaign_11758" />
		<textfield id="campaign_name" set="{@Campaign_Name}" jscriptevent="onkeydown" />
		<textfield id="campaign_name" jscriptevent="onkeyup" click="false" />
		<button value="Save" />
	</form>
	<script file="{@global.scriptsdir}/modules/lib/Campaign_Delete_Lib.xml" />	
	
	<!-- Search for this record -->
	<link id="moduleTab_Accounts" />
	<div class="moduleTitle">
		<link href="index.php?module=Accounts&amp;action=index" click="false" />
	</div>
	<button id="search_form_clear" />
	<textfield id="name_basic" set="Account_Campaign_11758" />
	<button id="search_form_submit" />
	<table class="list view">
		<link text="Account_Campaign_11758" />
	</table>
	<button id="edit_button" />
	<button id="SAVE" />
	<browser assertnot="{@Campaign_Name}" />
	
	<!--Delete the test data-->
	<script file="{@global.scriptsdir}/modules/lib/TargetList_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />

	<puts text="Test: Campaigns_11758.xml finished." />
</soda>
