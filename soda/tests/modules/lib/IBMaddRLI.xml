<soda>	<puts text="Lib: IBMaddRLI.xml starting." /> 	<button id="edit_button" />		<csv file="{@global.scriptsdir}/modules/lib/IBMaddRLI.csv" var="IBMaddRLI">		<div id="LBL_REVENUE_LINE_ITEM">			<link text="+ Add another" />		</div>		<textfield id="offering_{@IBMaddRLI.id}-input" append="le" />		<wait timeout="3" />		<li text="{@IBMaddRLI.offering}" click="true" />		<wait timeout="2" />				<textfield id="revenue_amount_{@IBMaddRLI.id}" set="{@IBMaddRLI.amount}" />		<select id="probability_{@IBMaddRLI.id}" set="{@IBMaddRLI.probability}" />		<!-- Select owner -->		<button id="btn_assigned_user_name_{@IBMaddRLI.id}" />		<button id="search_form_clear" />		<textfield id="user_name_advanced" set="{@IBMaddRLI.owner}" />		<button id="search_form_submit" />		<link text="{@IBMaddRLI.owner}" />				<textfield id="decision_date_{@IBMaddRLI.id}" set="{@IBMaddRLI.date}" />	</csv>		<button id="SAVE_FOOTER" />	<puts text="Lib: IBMaddRLI.xml finished." /> </soda>