<soda>	<puts text="Lib: IBMaddRLI.xml starting." /> 	<csv file="{@global.scriptsdir}/modules/lib/IBMaddRLI.csv" var="IBMaddRLI">		<button id="opportunities_ibm_revenuelineitems_create_button" />		<textfield id="level_search-input" append="{@IBMaddRLI.search_str}" />		<wait timeout="3" />		<li text="/{@IBMaddRLI.select_str}/" click="true" />		<wait timeout="3" />		<select id="roadmap_status" set="{@IBMaddRLI.roadmap_status}" />		<select id="probability" set="{@IBMaddRLI.probability}" />		<textfield id="revenue_amount" set="{@IBMaddRLI.revenue_amount}" />		<button value="Save" />	</csv>	<browser assert="{@IBMaddRLI.select_str}" />	<puts text="Lib: IBMaddRLI.xml finished." /> </soda>