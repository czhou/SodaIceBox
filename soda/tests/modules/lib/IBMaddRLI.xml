<soda>	<puts text="Lib: IBMaddRLI.xml starting." /> 	<csv file="{@global.scriptsdir}/modules/lib/IBMaddRLI.csv" var="IBMaddRLI">		<button id="opportunities_ibm_revenuelineitems_create_button" />		<textfield id="level_search-input" append="{@IBMaddRLI.search_str}" />		<li text="{@IBMaddRLI.select_str}" click="true" />		<select id="roadmap_status" set="{@IBMaddRLI.roadmap_status}" />		<select id="probability" set="{@IBMaddRLI.probability}" />		<textfield id="revenue_amount" set="{@IBMaddRLI.revenue_amount}" />		<button value="Save" />	</csv>	<puts text="Lib: IBMaddRLI.xml finished." /> </soda>