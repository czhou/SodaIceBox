        <!--textfield id='date_quote_expected_closed_advanced' set='3/22/2010' /-->
        <button id='search_form_submit'/>
    </form>

    <table class='list view'>
        <browser assert='{@pre_Quote_name}'/>
        <link text='{@pre_Quote_name}'/>
    </table>
    <browser assert='Â»{@pre_Quote_name}'/>

    <!--Set search form back to Basic Search-->
    <link id='moduleTab_Quotes'/>
    <browser assert='Quotes'/>
    <form id='search_form'>
        <link id="basic_search_link" required='false'/>
        <wait timeout='2'/>
        <button id='search_form_clear'/>
	<button id="search_form_submit"/>
    </form>
    <puts text='---Quote_2506.xml---Finished!'/>
