<soda>
    <puts text='---Quote_2506.xml: Search Quote: Verify that existing quote can be searched when using advanced search function.---Start!'/>
    <link id='moduleTab_Quotes'/>
    <browser assert='Quotes'/>
    <form id='search_form'>
        <link id="advanced_search_link" required='false'/>
        <wait timeout='2'/>
        <button id='search_form_clear'/>
        <textfield id='name_advanced' set='{@pre_Quote_name}' />
        <select name='quote_stage_advanced[]' set='{@Quote_Create_Lib.quote_stage}' />
        <!--select name='quote_stage_advanced[]' set='Negotiation' /-->
