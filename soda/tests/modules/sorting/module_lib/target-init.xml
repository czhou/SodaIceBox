
<soda>
	<!-- create Target records -->
	<link id="moduleTab_Prospects" />
	<csv file="{@global.scriptsdir}/csvs/sorting/Target_Listview.csv" 
      var="Target_Listview_Sort">
		
		<link text="Create Target" />
		<browser assert="Description" />

		<form id="EditView">
			<timestamp />
			<textfield id="last_name" set="{@Target_Listview_Sort.name}" /> 
			<textfield id="title" set="{@Target_Listview_Sort.title}" />
			<textfield id="phone_work" set="{@Target_Listview_Sort.phone} " />
			<button id="SAVE_HEADER" />
		</form>
		<div class="moduleTitle">
			<link href="index.php?module=Prospects&amp;action=index" click="false" />
			<browser assert="{@Target_Listview_Sort.name}" />
		</div>
	</csv>
</soda>
