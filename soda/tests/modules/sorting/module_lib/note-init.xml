
<soda>
	<puts text="note-init.xml starting." />
	<link id="moduleTab_Notes" />
	
	<csv file="{@global.scriptsdir}/csvs/sorting//note_Listview.csv" var="note_list_sort">
		<link text="Create Note or Attachment" />

		<form id="EditView">
			<textfield id="name" set="{@note_list_sort.name}" />
			<filefield id="filename_file" set="{@global.scriptsdir}/csvs/sorting/lotuslive/{@note_list_sort.filename_file}" />
			<!--contact select -->
			<button id="btn_contact_name" />
		</form>
		<textfield id="last_name_advanced" set="{@note_list_sort.contact}" /> 	
		<button value="Search" />
		<link text="{@note_list_sort.contact}" />
		
		<form id="EditView">
			<button id="SAVE" />
		</form>
		<wait timeout="2" />
		<button id="edit_button" click="false" />
		<browser assert="{@note_list_sort.name}" />
	</csv>

	<puts text="note-init.xml finished." />
</soda>

