
<soda>
	<puts text="call-init.xml starting." />
	<timestamp />
	<link id="moduleTab_Calls" />
	<csv file="{@global.scriptsdir}/csvs/sorting//Call_Listview.csv" var="call_list_sort">
		<link text="Log Call" />

		<form id="EditView">
			<textfield id="name" set="{@call_list_sort.name}" />
			<select id="direction" set="{@call_list_sort.direction}" />
			<textfield id="date_start_date" set="{@call_list_sort.date_start_date}" />
			<select id="date_start_hours" set="10" />
			<select id="date_start_minutes" set="30" />
			<select id="parent_type" set="{@call_list_sort.parent_type}" />
			<!--account select -->
			<button id="btn_parent_name" />
		</form>
		<textfield id="last_name_advanced" set="{@call_list_sort.contact}" /> 	
		<button value="Search" />
		<link text="{@call_list_sort.contact}" />
		
		<form id="EditView">
			<button value="Save" />
		</form>
		<wait timeout="2" />
		<button id="edit_button" click="false" />
		<browser assert="{@call_list_sort.name}" />
	</csv>

	<puts text="call-init.xml finished." />
</soda>

