
<soda>
	<puts text="task-init.xml dueing." />
	<timestamp />
	<link id="moduleTab_Tasks" />
	<csv file="{@global.scriptsdir}/csvs/sorting//task_Listview.csv" var="task_list_sort">
		<link text="Create Task" />

		<form id="EditView">
			<textfield id="name" set="{@task_list_sort.name}" />
			<textfield id="date_due_date" set="{@task_list_sort.date_due_date}" />
			<select id="date_due_hours" set="10" />
			<select id="date_due_minutes" set="30" />
			<select id="status" set="{@task_list_sort.status}" />
			<!--contact select -->
			<button id="btn_contact_name" />
		</form>
		<textfield id="last_name_advanced" set="{@task_list_sort.contact}" /> 	
		<button value="Search" />
		<link text="{@task_list_sort.contact}" />
		
		<form id="EditView">
			<button id="SAVE" />
		</form>
		<wait timeout="2" />
		<button id="edit_button" click="false" />
		<browser assert="{@task_list_sort.name}" />
	</csv>

	<puts text="task-init.xml finished." />
</soda>

