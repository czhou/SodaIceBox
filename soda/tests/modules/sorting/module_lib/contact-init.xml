
<soda>
	<puts text="contact-init.xml starting." />
	<timestamp />
	<link id="moduleTab_Contacts" />
	<csv file="{@global.scriptsdir}/csvs/sorting//Contact_Listview.csv" var="contact_list_sort">
		<link text="Create Contact" />

		<form id="EditView">
			<textfield id="last_name" set="{@contact_list_sort.last_name}" />
			<textfield id="title" set="{@contact_list_sort.title}" />
			
			<!--account select -->
			<button id="btn_account_name" />
		</form>
		<textfield id="name_advanced" set="{@contact_list_sort.account_name}" /> 	
		<button value="Search" />
		<link text="{@contact_list_sort.account_name}" />
		
		<form id="EditView">
			<textfield id="phone_work" set="{@contact_list_sort.phone_office}" />
			<textfield id="primary_address_city" set="{@contact_list_sort.city}" />
			<textfield id="primary_address_state" set="{@contact_list_sort.state}" />
			<button id="SAVE_HEADER"/>
		</form>
		<wait timeout="2" />
		<button id="edit_button" click="false" />
		<browser assert="{@contact_list_sort.last_name}" />
	</csv>
	<var var="pre_contact_sort" set="{@contact_list_sort.last_name}" />
	<puts text="contact-init.xml finished." />
</soda>

