
<soda>
	<!-- create Campaign records -->
	<link id="moduleTab_Campaigns" />
	<csv file="{@global.scriptsdir}/csvs/sorting/Campaign_Listview.csv" 
      var="Campaign_Listview_Sort">
		
		<link text="Create Campaign (Classic)" />

		<form id="EditView">
			<timestamp />
			<textfield id="name" set="{@Campaign_Listview_Sort.name}" /> 
			<select id="status" set="{@Campaign_Listview_Sort.status}" />
			<textfield id="end_date" set="{@Campaign_Listview_Sort.end_date} " />
			<select id="campaign_type" set="{@Campaign_Listview_Sort.type}" />
			<button id="SAVE_HEADER" />
		</form>
		<div class="moduleTitle">
			<link href="index.php?module=Campaigns&amp;action=index" click="false" />
			<browser assert="{@Campaign_Listview_Sort.name}" />
		</div>
	</csv>
</soda>
