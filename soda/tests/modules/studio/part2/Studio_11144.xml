
<soda>
    
    <!--Show accept column after customized dashlet listview layout-->
    <puts text="Test: Studio_11144.xml starting" />
    <csv file="{@global.scriptsdir}/csvs/studio/Studio_11144.csv" var="studio11144">
	    <link text="Admin" />
	    <link text="Studio" assertPage="false" />
	    <link text="{@studio11144.module}" class="studiolink" assertPage="false" />
	    <link text="Layouts" class="studiolink" assertPage="false" />
	    <link text="Sugar Dashlet" class="studiolink" assertPage="false" />
	    <link text="Sugar Dashlet ListView" class="studiolink" assertPage="false" />
	    <table id="editor-content">
	    	<button id="savebtn" />
	    	<wait timeout="5" />
	    </table>
	    <link id="moduleTab_Home" />
	    <wait timeout="3" />
	    <link id="add_dashlets" />
		<link text="My {@studio11144.module}" />
		<div class="container-close" click="true" />
		<wait timeout="1" />
		<ul id="col_0_0">
			<browser assert="Accept?" />
		</ul>
		<div class="dashletToolSet" index="1">
			<link index="3" alert="true" />
		</div>	
	</csv>
    
    <puts text="Test: Studio_11144.xml finished." />
</soda>
